{
  "name": "vue-next-admin",
  "version": "1.0.0",
  "license": "ISC",
  "author": "jacquinc",
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=9.12.3"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --mode dev --host",
    "dev:joint": "vite --mode joint --host",
    "dev:beta": "vite --mode beta --host",
    "build:dev": "vite build --mode  dev",
    "build:test": "vite build --mode test",
    "build:prd": "vite build --mode prd",
    "build:prd-zip": "npm run build:prd && npm run zip-rmdist",
    "zip-rmdist": "cd dist/ && bestzip ../dist.zip * && cd .. && del-cli --force ./dist",
    "preview": "yarn run build && vite preview ",
    "prepare": "cd .. && husky install vue-next-admin/.husky",
    "lint": "eslint --ext .js,.jsx,.vue,.ts,.tsx src --fix",
    "style": "stylelint \"src/**/*.(vue|scss|css)\" --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "src/**/*.{vue,js,ts,jsx,tsx}": "eslint --fix",
    "src/**/*.{css,scss,vue}": [
      "stylelint  --fix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": ["svg-baker", "postcss"]
    }
  },
  "peerDependencies": {
    "vue": "3.4.27"
  },
  "dependencies": {
    "@element-plus/icons-vue": "0.2.4",
    "@fingerprintjs/fingerprintjs": "^4.0.1",
    "@ssthouse/vue-tree-chart": "^0.6.5",
    "@vue/cli-service": "^5.0.8",
    "@vue/shared": "^3.2.37",
    "@vueuse/core": "^7.7.0",
    "axios": "1.7.4",
    "countup.js": "^2.3.0",
    "dayjs": "^1.10.7",
    "echarts": "5.1.2",
    "element-plus": "2.8.7",
    "jquery": "^3.6.0",
    "js-cookie": "2.2.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "path": "^0.12.7",
    "path-to-regexp": "8.0.0",
    "pinia": "2.2.4",
    "print-js": "^1.6.0",
    "qs": "^6.9.3",
    "screenfull": "4.2.0",
    "vue": "3.4.27",
    "vue-echarts": "6.0.0",
    "vue-router": "4.0.12"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@stylistic/eslint-plugin-js": "^2.12.1",
    "@tinymce/tinymce-vue": "^4.0.5",
    "@types/echarts": "^4.9.7",
    "@types/lodash": "^4.14.180",
    "@types/lodash-es": "^4.17.7",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^15.14.9",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-vue": "4.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/compiler-sfc": "3.2.26",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.0.0-rc.18",
    "array-to-tree": "^3.3.2",
    "auto-import-types": "0.0.4",
    "bestzip": "^2.2.1",
    "better-scroll": "^2.4.2",
    "c8": "^7.11.3",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "default-passive-events": "^2.0.0",
    "del-cli": "^4.0.1",
    "element-resize-detector": "^1.2.4",
    "esbuild": "^0.17.19",
    "eslint": "8.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-define-config": "1.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-plus": "^1.4.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-vue": "8.1.1",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "lint-staged": "^12.4.1",
    "mockjs": "^1.1.0",
    "postcss": "8.4.31",
    "postcss-html": "1.3.0",
